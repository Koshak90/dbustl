prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: dbustl glib binding
Description: Dbus template library, a helper C++ library for the Free desktop message bus
Version: @VERSION@
Libs: -L${libdir} -ldbustl-glib-1 -ldbustl-1 @DBUS_LIBS@ @GLIB_LIBS@
Cflags: -I${includedir} @DBUS_CFLAGS@ @GLIB_CFLAGS@
