// STL list support

#ifndef DBUSTL_TYPES_LIST
#define DBUSTL_TYPES_LIST

#include <list>
#include <dbustl-1/types/StdTools>

namespace dbustl {
namespace types {

template <typename T>
struct Signature<std::list<T> > : public ArraySignature<T> {};

template<typename T>
struct Serializer<std::list<T> >: public ArraySerializer<std::list<T> > {};
template<typename T>
struct Deserializer<std::list<T> >: public ArrayDeserializer<std::list<T> > {};

}
}

#endif /* DBUSTL_TYPES_LIST */
