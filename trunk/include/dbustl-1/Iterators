#ifndef DBUSTL_ITERATORS
#define DBUSTL_ITERATORS

#define DBUSTL_ITERATE_COMMA_1(f) \
    f(1)
#define DBUSTL_ITERATE_SEMICOLON_1(f) \
    f(1)

#define DBUSTL_ITERATE_COMMA_2(f) \
    DBUSTL_ITERATE_COMMA_1(f), f(2)
#define DBUSTL_ITERATE_SEMICOLON_2(f) \
    DBUSTL_ITERATE_SEMICOLON_1(f); f(2)

#define DBUSTL_ITERATE_COMMA_3(f) \
    DBUSTL_ITERATE_COMMA_2(f), f(3)
#define DBUSTL_ITERATE_SEMICOLON_3(f) \
    DBUSTL_ITERATE_SEMICOLON_2(f); f(3)

#define DBUSTL_ITERATE_COMMA_4(f) \
    DBUSTL_ITERATE_COMMA_3(f), f(4)
#define DBUSTL_ITERATE_SEMICOLON_4(f) \
    DBUSTL_ITERATE_SEMICOLON_3(f); f(4)

#define DBUSTL_ITERATE_COMMA_5(f) \
    DBUSTL_ITERATE_COMMA_4(f), f(5)
#define DBUSTL_ITERATE_SEMICOLON_5(f) \
    DBUSTL_ITERATE_SEMICOLON_4(f); f(5)

#define DBUSTL_ITERATE_COMMA_6(f) \
    DBUSTL_ITERATE_COMMA_5(f), f(6)
#define DBUSTL_ITERATE_SEMICOLON_6(f) \
    DBUSTL_ITERATE_SEMICOLON_5(f); f(6)

#define DBUSTL_ITERATE_COMMA_7(f) \
    DBUSTL_ITERATE_COMMA_6(f), f(7)
#define DBUSTL_ITERATE_SEMICOLON_7(f) \
    DBUSTL_ITERATE_SEMICOLON_6(f); f(7)

#define DBUSTL_ITERATE_COMMA_8(f) \
    DBUSTL_ITERATE_COMMA_7(f), f(8)
#define DBUSTL_ITERATE_SEMICOLON_8(f) \
    DBUSTL_ITERATE_SEMICOLON_7(f); f(8)

#endif /* DBUSTL_ITERATORS */
